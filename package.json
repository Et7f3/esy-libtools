{
  "name": "esy-libtools",
  "version": "2.4.6000",
  "description": "esy-libtools packaged for esy",
  "esy": {
    "buildsInSource": true,
    "exportedEnv": {
      "PKG_CONFIG_PATH": {
        "scope": "global",
        "val": "#{self.lib / 'pkgconfig' : $PKG_CONFIG_PATH }"
      }
    },
    "build": [
      "./bootstrap",
      "./configure --prefix=#{self.install} #{os == 'windows' ? '--host=x86_64-w64-mingw32' : ''}",
      "make",
      "make install"
    ]
  },
  "dependencies": {
    "autoconf": "esy-packages/esy-autoconf#ea58671a08c7ebf89b0cebd41f4950ca67157b01",
    "automake": "ulrikstrid/esy-automake#10779a9ca0cb23e60fa619e1e0b5ed03b9c3e575"
  },
  "resolutions": {}
}
